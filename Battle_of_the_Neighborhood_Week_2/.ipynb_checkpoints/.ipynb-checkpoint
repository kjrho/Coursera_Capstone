   "source": [
    "#join school ranking to the sales data df\n",
    "district = sales.join(srank.set_index('DistrictName'), on='SCHOOLDESC', how='inner')\n",
    "district = district.sort_values(by=['Rank', 'SALEPRICE'])\n",
    "d =district[['PROPERTYZIP','SCHOOLCODE','SCHOOLDESC','FINISHEDLIVINGAREA','LOTAREA','BEDROOMS','FULLBATHS','SALEPRICE','LAT','LNG','PRICEPERSQFT','Rank']].copy()\n",
    "d['SALEPRICEDIV']=round(d['SALEPRICE'].fillna(0).astype('int')/1000,0)\n",
    "d['LIVINGAREA'] =round( d['FINISHEDLIVINGAREA'].fillna(0).astype('int')/1000,0)\n",
    "d['PROPERTYZIP']=d['PROPERTYZIP'].astype('int')\n",
    "d.head()"
   ]
