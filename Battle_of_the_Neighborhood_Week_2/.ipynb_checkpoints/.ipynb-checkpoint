    "import folium\n",
    "map_ac = folium.Map\n",
    "\n",
    "map_ac = folium.Map(location=[latitude, longitude], zoom_start=10)\n",
    "map_ac.choropleth(\n",
    " geo_data= county_geo,\n",
    " data=sub2,\n",
    " columns=['SCHOOLDESC', 'PRICEPERSQFT'],\n",
    " key_on='feature.properties.SCHOOLD',\n",
    " fill_color='YlGn',\n",
